<core:FragmentDefinition
        xmlns="sap.m"
        xmlns:core="sap.ui.core"
        xmlns:u="sap.ui.unified">
    <u:Menu>
        <u:MenuItem
                text="Users"
                select="handleMenuItemPress"
                icon="sap-icon://employee"/>
        <u:MenuItem
                text="Rooms"
                select="handleMenuItemPress"
                icon="sap-icon://building"/>
        <u:MenuItem
                text="Settings"
                icon="sap-icon://settings">
            <u:Menu>
                <u:MenuItem
                        text="1st Sub Item"
                        select="handleMenuItemPress"/>
                <u:MenuItem
                        text="2nd Sub Item"
                        select="handleMenuItemPress"/>
                <u:MenuItem
                        text="3rd Sub Item but inactive"
                        enabled="false"/>
            </u:Menu>
        </u:MenuItem>

        <u:MenuItem
                text="Actions"
                startsSection="true"
                select="handleMenuItemPress"
                icon="sap-icon://action"/>

    </u:Menu>

</core:FragmentDefinition>